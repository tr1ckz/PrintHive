{
  "name": "printhive",
  "version": "1.3.5",
  "description": "Web application for 3D printer management - monitor printers, manage models and timelapses",
  "main": "simple-server.js",
  "scripts": {
    "start": "node simple-server.js",
    "dev": "nodemon simple-server.js",
    "dev:frontend": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "reset-admin": "node reset-admin.js",
    "docker:build": "docker build -t printhive:latest .",
    "docker:run": "docker run -d --name printhive -p 3000:3000 -v printhive_data:/app/data -v printhive_library:/app/library -v printhive_sessions:/app/sessions printhive:latest"
  },
  "keywords": [
    "printhive",
    "3d-printer",
    "timelapse",
    "printer-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@napi-rs/canvas": "^0.1.88",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.8.1",
    "basic-ftp": "^5.1.0",
    "better-sqlite3": "^12.6.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-session": "^1.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "ftp": "^0.3.10",
    "http-proxy-middleware": "^3.0.5",
    "jszip": "^3.10.1",
    "mqtt": "^5.14.1",
    "multer": "^2.0.2",
    "openid-client": "^6.8.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-openidconnect": "^0.1.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "session-file-store": "^1.5.0",
    "ssh2-sftp-client": "^12.0.1",
    "tar": "^7.5.2",
    "three": "^0.182.0",
    "threejs-webworker-3mf-loader": "github:manyfold3d/threejs-webworker-3mf-loader",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "overrides": {
    "async": "^3.2.6",
    "cross-spawn": "^7.0.6",
    "glob": "^11.0.0",
    "brace-expansion": "^2.0.1"
  }
}
